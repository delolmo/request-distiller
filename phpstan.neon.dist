parameters:
    level: max
    paths:
        - src
        - tests
    ignoreErrors:
        -
            message: '#Call to an undefined method [a-zA-Z0-9\\_]+::getAttributes\(\)#'
            path: %currentWorkingDirectory%/src/Extractor/AttributesExtractor.php
        -
            message: '#Call to an undefined method [a-zA-Z0-9\\_]+::getParsedBody\(\)#'
            path: %currentWorkingDirectory%/src/Extractor/ParsedBodyExtractor.php
        -
            message: '#Call to an undefined method [a-zA-Z0-9\\_]+::getQueryParams\(\)#'
            path: %currentWorkingDirectory%/src/Extractor/QueryParamsExtractor.php
        -
            message: '#Call to an undefined method Zend\\Filter\\FilterInterface::attach\(\)#'
            path: %currentWorkingDirectory%/src/Distiller.php
        -
            message: '#Call to an undefined method Zend\\Validator\\ValidatorInterface::attach\(\)#'
            path: %currentWorkingDirectory%/src/Distiller.php
includes:
    - vendor/phpstan/phpstan-phpunit/extension.neon